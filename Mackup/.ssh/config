Include ~/.orbstack/ssh/config

Host github
	User git
	Port 22
	Hostname github.com
	IdentityFile ~/.ssh/id_rsa
	IdentityFile ~/.ssh/id_ecdsa_prive
	IdentityFile ~/.ssh/id_ed25519_work
	IdentityFile ~/.ssh/linode_ssh_github
	IdentitiesOnly yes

Host *
	IgnoreUnknown UseKeychain,AddKeysToAgent
	Port 22
	UseKeychain yes
	AddKeysToAgent yes
	IdentityFile ~/.ssh/id_rsa
	IdentityFile ~/.ssh/id_ecdsa_prive
